          "intervalFactor": 1,
          "query": "SELECT Dest, Flights, DelayPercentage, dest.Name, dest.Latitude, dest.Longitude \r\nFROM \r\n(\r\n  SELECT Dest, count() AS Flights, sum(ArrDel15)/Flights*100.0 as DelayPercentage FROM ontime\r\n  WHERE $timeFilter \r\n  GROUP BY Dest\r\n) AS o \r\nJOIN airports AS dest ON o.Dest = dest.IATA \r\n",
          "rawQuery": "/* grafana dashboard=Ontime Database Carrier and Airport Stats, user=1 */\nSELECT Dest, Flights, DelayPercentage, dest.Name, dest.Latitude, dest.Longitude,\r\nmultiIf(DelayPercentage < 3, 'green', DelayPercentage < 10, 'yellow', 'red') AS Color \r\nFROM \r\n(\r\n  SELECT Dest, count() AS Flights, sum(ArrDel15)/Flights*100.0 as DelayPercentage FROM ontime\r\n  WHERE toDateTime(FlightDate) >= toDateTime(1577865600) AND toDateTime(FlightDate) <= toDateTime(1609488000) \r\n  GROUP BY Dest\r\n) AS o \r\nJOIN airports AS dest ON o.Dest = dest.IATA",
          "refId": "A",
          "round": "0s",
          "showFormattedSQL": true,
          "skip_comments": true,
          "table": "ontime"
