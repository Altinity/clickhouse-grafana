{
  "name": "altinity-clickhouse-grafana",
  "version": "3.4.0",
  "description": "Altinity grafana datasource for ClickHouse",
  "scripts": {
    "build": "npm run build:wasm && npm run build:frontend",
    "dev": "npm run build:wasm && webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker compose up --build -d grafana",
    "sign": "bash -xec \"source ./.release_env && npx --yes @grafana/sign-plugin@latest\"",
    "create_instrumented": "npx nyc instrument --source-map --nycrc-path .nycrc ./src ./instrumented",
    "build:test:frontend": "webpack -c ./webpack.config.instrumented.ts --env test",
    "build:frontend": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:backend": "docker compose run --rm backend_builder && chmod +rx ./dist",
    "build:wasm": "GOARCH=wasm GOOS=js tinygo build --target wasm ./pkg/backend_wasm.go && gzip -f ./backend_wasm.wasm && mkdir -pv ./src/static/ && mv -fv ./backend_wasm.wasm.gz ./src/static/backend.wasm.gz"
  },
  "author": "Altinity",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e-selectors": "^11.2.2",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc/core": "1.3.75",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.15.11",
    "@types/pako": "^2.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "nyc": "^17.1.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.4.0",
    "source-map-support": "^0.5.21",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@grafana/data": "11.5.2",
    "@grafana/runtime": "11.5.2",
    "@grafana/ui": "11.5.2",
    "brotli-wasm": "^3.0.1",
    "browserify-fs": "^1.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fs": "^0.0.1-security",
    "luxon": "^3.5.0",
    "pako": "^2.1.0",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "rxjs": "^7.8.1",
    "tslib": "2.5.3"
  },
  "packageManager": "npm@9.8.1"
}
