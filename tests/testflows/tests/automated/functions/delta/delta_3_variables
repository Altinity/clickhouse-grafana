$delta(first_variable, second_variable, third_variable) from table_name
SELECT t, runningDifference(max_0) AS max_0_Delta, runningDifference(max_1) AS max_1_Delta, runningDifference(max_2) AS max_2_Delta FROM ( SELECT (intDiv(toUInt32(undefined), 30) * 30) * 1000 AS t, max(first_variable) AS max_0, max(second_variable) AS max_1, max(third_variable) AS max_2 from table_name WHERE