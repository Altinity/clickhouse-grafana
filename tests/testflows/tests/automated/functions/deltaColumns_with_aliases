$deltaColumns(key as k, value as v) FROM table_name
SELECT t, groupArray((k, max_0_Delta)) AS groupArr FROM ( SELECT t, k, if(neighbor(k,-1,k) != k, 0, runningDifference(max_0)) AS max_0_Delta FROM ( SELECT (intDiv(toUInt32(undefined), 30) * 30) * 1000 AS t, key as k, max(value as v) AS max_0 FROM table_name WHERE